translate portuguese b1c04zs04_d9874d98:

    # mo "You like a man who's consistent, huh?"
    mo "Você gosta de um homem consistente, né?"

# game/code/Book-1/Chapter-4z/s04.rpy:160
translate portuguese b1c04zs04_adc70adc:

    # pm "Shut up and shoot again."
    pm "Cala a boca e atira de novo."

# game/code/Book-1/Chapter-4z/s04.rpy:164
translate portuguese b1c04zs04_00c90500:

    # mct "Slow down, make sure I have proper form."
    mct "Devagar, certificar de estar na postura correta."

# game/code/Book-1/Chapter-4z/s04.rpy:165
