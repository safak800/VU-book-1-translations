translate spanish b1c06cs06_023842eb:

    # nv "You're the best."
    nv "Eres el mejor."

# game/code/Book-1/Chapter-6c/s06.rpy:547
translate spanish b1c06cs06_fba91fc6:

    # ad "Who's next?"
    ad "¿Quién es el siguiente?"

